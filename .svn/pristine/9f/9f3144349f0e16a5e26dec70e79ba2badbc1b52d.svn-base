table.dataTable thead .sorting_asc {
    background: none;
}

table.dataTable thead > tr > th.sorting_asc, table.dataTable thead > tr > th.sorting_desc, table.dataTable thead > tr > th.sorting, table.dataTable thead > tr > td.sorting_asc, table.dataTable thead > tr > td.sorting_desc, table.dataTable thead > tr > td.sorting {
    padding-right: 0px;
    position: relative;
}

table.dataTable thead .sorting_asc span {
    top: -18px;
    position: relative;
}

.rule-help {
    float: left;
    font-size: 12px;
    margin-right: 12px;
    line-height: 32px;
    position: relative;
}

.rule-help span {
    cursor: pointer;
}

.rule-help:hover span {
    color: #f7b659;
}

.rule-help-wrap {
    position: absolute;
    width: 400px;
    border: 1px solid #f7b659;
    background: #fffdf7;
    border-radius: 4px !important;
    overflow: hidden;
    right: 0px;
    padding: 10px;
    z-index: 99999999;
    display: none;
}

.rule-help-wrap p {
    padding: 0px;
    margin: 0px;
    font-size: 13px;
    line-height: 24px;
}

.rule-help:hover .rule-help-wrap {
    display: block;

}

.section-box {
    width: 100%;
    float: left;
}

.section-step {
    padding: 20px 10%;
    background-color: #f0f8f4;
}

.ant-steps-tail {
    position: relative;
    display: inline-block;
    vertical-align: top;
}

.ant-steps-tail p {
    border: 1px solid #415161;
    background-color: #fff;
    border-radius: 20px !important;
    width: 140px;
    height: 34px;
    margin: 0px;
    line-height: 34px;
    text-align: center;
    position: relative;
    z-index: 99;
}

.ant-click p {
    color: #fff;
    background-color: #415161;
}

.ant-steps-tail i {
    position: absolute;
    width: 100%;
    top: 17px;
    height: 1px;
    background: #cdd9e6;
    display: block;
}

.ant-steps-tail i em {
    background: #415161;
    display: block;
    position: absolute;
    width: 0px;
    top: 0px;
    height: 1px;
    display: block;
    transition-duration: 0.3s;
    transition-property: width;
    transition-timing-function: linear;
}


.text-muted {
    font-size: 12px;
}

.ant-animt i em {
    background: #415161;
    display: block;
    position: absolute;
    width: 100%;
    top: 0px;
    height: 1px;
    display: block;
    transition-duration: 0.3s;
    transition-property: width;
    transition-timing-function: linear;
}

.steps-action {
    /*float: right;*/
}

.ant-btn {
    display: inline-block;
    margin-bottom: 0;
    font-weight: 500;
    text-align: center;
    -ms-touch-action: manipulation;
    touch-action: manipulation;
    cursor: pointer;
    background-image: none;
    border: 1px solid transparent;
    white-space: nowrap;
    line-height: 1.5;
    padding: 0 10px;
    font-size: 12px;
    height: 28px;
    font-weight: 400;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    transition: all .3s cubic-bezier(.645, .045, .355, 1);
    position: relative;
    color: #415161;
    background-color: #eee;
    border-color: #d9d9d9;
}

.ant-btn-ghost {

}

.ant-btn-primary {
    color: #fff;
    background-color: #4dbd74;
    border-color: #4dbd74;
}

.ant-btn-ready-click {
    display: none;
    color: #fff;
    background-color: #4bd0b6;
    border-color: #4bd0b6;
}

.ordButton {
    display: inline-block;
    margin-bottom: 0;
    font-weight: 500;
    text-align: center;
    -ms-touch-action: manipulation;
    touch-action: manipulation;
    cursor: pointer;
    background-image: none;
    border: 1px solid transparent;
    white-space: nowrap;
    line-height: 1.5;
    padding: 0 15px;
    font-size: 14px;
    border-radius: 17px;
    height: 28px;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    transition: all .3s cubic-bezier(.645, .045, .355, 1);
    position: relative;
    border-radius: 3px !important;
    color: #fff;
    display: none;
    background-color: #4bd0b6;
    border-color: #4bd0b6;
}

.section-conts {
