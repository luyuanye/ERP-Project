	/*选项卡 start*/	
	.ERPtabbox{width:100%;height: 100%;}
	.ERPtabbox .tabErp{overflow:hidden;border-bottom: 1px solid #ccc;}
	.ERPtabbox .tabErp a.tabErpA{display:block;padding:8px 10px;float:left;text-decoration:none;color:#333;border-bottom: 2px solid transparent; margin-right: 10px;}
	.ERPtabbox .tabErp a.tabErpA:hover{color:#13C19F;}
	.ERPtabbox .tabErp a.tabErpA.on{color:#13C19F;border-color: #13C19F;}	
	.ERPtabbox .contentErpbox{overflow:hidden;width:100%;height:calc(100% - 41px);position:relative;}
	.ERPtabbox .contentErpbox >ul.contentErpboxUL{position:absolute;left:0;top:0;height: 100%;width: 400%;}
	.ERPtabbox .contentErpbox >ul.contentErpboxUL >li{height:100%;float:left;width: 25%;overflow-x: hidden;}
	/*选项卡 end*/
