/**************************列表下级子页面共有部分*/
.fixedFOOTER{position: fixed;width: 100%;bottom: 33px;background: #fff;}
.fixedFOOTER .form-actions{line-height: 53px;padding: 0px 20px;box-shadow: 0 0 8px 5px #eee;}
.page-boxsBOX{width:100%;height:100%;overflow-x: hidden;}/*没有下面固定保存按钮的时候需要他来做滚动*/
	.page-boxs{width:100%;height:calc(100% - 55px);overflow-x: hidden;}
	.page-boxs .page-titles{border-bottom:1px solid #eee!important;}
	.page-boxs .page-titles p.headerTitle,.page-boxs .page-wraps p.headerTitle{font-size: 16px;margin: 0;padding: 10px 15px;background: #f0f8f4;color: #3c9765;}
