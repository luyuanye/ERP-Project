<!--<p>订单/退货单</p>-->
<link href="../assets/global/css/plugins.min.css" rel="stylesheet" type="text/css">
<div class="portlet light">
    <div class="portlet-title">
        <form class="form-inline">
            <div class="pull-left">
                <div class="btn-group pull-left">
                    <button class="btn green  btn-outline dropdown-toggle" data-toggle="dropdown">全部退单
                        <i class="fa fa-angle-down"></i>
                    </button>
                    <ul class="dropdown-menu pull-right">
                        <li><a href="javascript:;">全部退单</a></li>
                        <li><a href="javascript:;">待处理退单</a></li>
                        <li><a href="javascript:;">未完成退单</a></li>
                        <li><a href="javascript:;">已完成退单</a></li>
                        <li><a href="javascript:;">已作废退单</a></li>
                    </ul>
                </div>
                <div class="form-group pull-left ml-10">
                    <input type="text" class="form-control input-medium pull-left" placeholder="请输入退单号/客户名称">
                    <button type="submit" class="btn green pull-left"><i class="fa fa-search"></i>搜索</button>
                </div>
            </div>
            <div class="form-group pull-right">
                <a class="btn btn-success"
                   onclick="addTab('新增退货单','Order/orderManagement/returnFormAdd.html','returnFormAdd','fa fa-plus',this)"
                   date-num="orderFormAdd"><i class="fa fa-plus"></i>新增</a>
            </div>
        </form>
    </div>
    <div class="portlet-body">
        <div id="returnFormTab_wrapper" class="dataTables_wrapper no-footer">
            <div class="top"></div>
            <table style="white-space: nowrap;text-overflow: ellipsis; overflow: hidden;width: 100%;"
                   class="table table-striped table-bordered table-hover table-checkable order-column table_example dataTable no-footer"
                   id="returnFormTab" role="grid" aria-describedby="returnFormTab_info">
                <thead>
                <tr role="row">
                    <th class="sorting_disabled" rowspan="1" colspan="1" aria-label="" style="min-width: 55px;">
                        <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
                            <input type="checkbox" class="group-checkable" data-set="#returnFormTab.checkboxes">
                        </label>
                    </th>
                    <th class="sorting_asc" tabindex="0" aria-controls="returnFormTab" rowspan="1" colspan="1"
                        aria-sort="ascending" aria-label="退单号: activate to sort column descending"
                        style="min-width: 170px;">退单号
                    </th>
                    <th class="sorting" tabindex="0" aria-controls="returnFormTab" rowspan="1" colspan="1"
                        aria-label="下单时间: activate to sort column ascending" style="min-width: 156px;">下单时间
                    </th>
                    <th class="sorting" tabindex="0" aria-controls="returnFormTab" rowspan="1" colspan="1"
                        aria-label="客户名称: activate to sort column ascending" style="min-width: 136px;">客户名称
                    </th>
                    <th class="sorting" tabindex="0" aria-controls="returnFormTab" rowspan="1" colspan="1"
                        aria-label="金额: activate to sort column ascending" style="min-width: 100px;">金额
                    </th>
                    <th class="sorting" tabindex="0" aria-controls="returnFormTab" rowspan="1" colspan="1"
                        aria-label="状态: activate to sort column ascending" style="min-width: 115px;">状态
                    </th>
                    <th class="sorting" tabindex="0" aria-controls="returnFormTab" rowspan="1" colspan="1"
                        aria-label="退款状态: activate to sort column ascending" style="min-width: 125px;">退款状态
                    </th>
                    <th class="sorting" tabindex="0" aria-controls="returnFormTab" rowspan="1" colspan="1"
                        aria-label="操作: activate to sort column ascending" style="min-width: 85px;">操作
                    </th>
                </tr>
                </thead>
                <tbody>
                <tr class="odd" role="row">
                    <td>
                        <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
                            <input type="checkbox" class="checkboxes" value="1">
                            <span></span>
                        </label>
                    </td>
                    <td>DH-O-20180710-361113</td>
                    <td>2018-07-10 14:24</td>
                    <td>lsy</td>
                    <td>0.00</td>
                    <td>待发货确认</td>
                    <td>已收款</td>
                    <td>
                        <div class="action-buttons f18">
                            <a  title="审核"
                                onclick="addTab('审核','Order/orderManagement/returnFormdetail.html','returnFormdetail','fa fa-shield',this)"
                                date-num="orderFormdetail"><i class="fa fa-shield"></i></a>
                            <a  title="退款详情"
                                onclick="addTab('退款详情','Order/orderManagement/returnFormdetail.html','returnFormdetail','fa fa-search',this)"
                                date-num="orderFormdetail"><i class="fa fa-search "></i></a>
                            <a  title="作废"
                                onclick="addTab('作废','Order/orderManagement/returnFormdetail.html','returnFormdetail','fa fa-pencil',this)"
                                date-num="orderFormdetail"><i class="fa fa-close"></i></a>
                        </div>
                    </td>
                </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
<script type="text/javascript">
    jQuery(function ($) {
        TableDatatablesManaged.init("#returnFormTab");
    })
</script>
<script type="text/javascript">
    $(function () {
        $(".date-picker").datepicker();
    })
</script>

