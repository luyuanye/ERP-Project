<script type="text/javascript">
	$(function() {
		self_adaption(); //初始调用formscroll()[form_model.js]
		$(window).resize(function() {
			self_adaption();			
			if ($(".page-content").width()>991) {				
		   	   $(".collapse").css("height","initial!important")	
			}
		}); //窗口拉动调用方法formscroll()[form_model.js]
	})

	function self_adaption() {
		var Htop = $(".page-header").outerHeight();
		var Hfooter = $(".page-footer").outerHeight();
		var newhh = $(window).height() - Htop - Hfooter;		
		$(".page-container").height(newhh);		
		$(".page-sidebar-wrapper").height(newhh);
		$(".page-content-wrapper").height(newhh);
		/*动态高table盒子*/
	}	 
</script>


